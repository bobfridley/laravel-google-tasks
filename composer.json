{
    "name": "bobfridley/laravel-google-tasks",
    "description": "Manage Google Tasks",
    "keywords": [
        "laravel",
        "framework",
        "Google Tasks",
        "GoogleTasks",
        "Bob Fridley",
        "BobFridley"
    ],
    "keywords": [
        "bobfridley",
        "laravel-google-tasks"
    ],
    "homepage": "https://github.com/bobfridley/laravel-google-tasks",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Fridley",
            "email": "robert.fridley@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "google/apiclient": "^1.1.7",
        "illuminate/support": "^5.2.8",
        "nesbot/carbon": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3.4",
        "orchestra/testbench": "^3.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "BobFridley\\GoogleTasks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BobFridley\\GoogleTasks\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
